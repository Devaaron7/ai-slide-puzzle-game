# Hugging Face API Token for image generation
REACT_APP_HF_TOKEN=your_hugging_face_token_here

# EmailJS Configuration
# Set to 'true' to enable email notifications
REACT_APP_EMAILJS_ENABLED=true
# Get these values from your EmailJS account
REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key
REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
